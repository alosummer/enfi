   RowClass="cux.oracle.apps.per.review.server.PerReviewGroupVORowImpl"
   ComponentClass="cux.oracle.apps.per.review.server.PerReviewGroupVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT rownum
     , rg.group_id
     , rg.org_id
     , rg.group_name
     , rg.flow_type
     , rg.epm_levelmode
     , rg.attribute1
     , rg.attribute2
     , cv.NAME         AS org_name
     , lm.meaning      AS epm_meaning
     , lm.DESCRIPTION  AS epm_desc
  FROM CUX_REVIEW_GROUP_T rg
     , cux_company_v cv
     , cux_epm_lm_v lm
 WHERE rg.org_id = cv.organization_id
   AND rg.epm_levelmode = lm.lookup_code
   ]]></SQLQuery>
