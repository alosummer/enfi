<ViewObject
   Name="RoleManageVO"
   OrderBy="rownum"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="cux.oracle.apps.per.review.server.RoleManageVORowImpl"
   ComponentClass="cux.oracle.apps.per.review.server.RoleManageVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT rownum,
       RoleManageEO.ROLE_ID, 
       RoleManageEO.GROUP_ID, 
       rg.group_name AS GROUP_NAME,
       RoleManageEO.ORG_ID, 
       cv.NAME AS ORG_NAME,
       RoleManageEO.PERSON_ID, 
       papf.last_name AS EMP_NAME,
       RoleManageEO.ROLE_CODE, 
       hr_contract_api.get_meaning(RoleManageEO.Attribute1, 'YES_NO') AS ATTRIBUTE1,
       RoleManageEO.LAST_UPDATE_DATE, 
       RoleManageEO.LAST_UPDATED_BY, 
       RoleManageEO.LAST_UPDATE_LOGIN, 
       RoleManageEO.CREATED_BY, 
       RoleManageEO.CREATION_DATE       
FROM CUX_GROUP_ROLE_T RoleManageEO,
     CUX_ORGANIZATION_V cv,
     CUX_REVIEW_GROUP_T rg,
     per_all_people_f    papf
WHERE RoleManageEO.Org_Id = cv.organization_id(+)
      AND RoleManageEO.Group_Id = rg.group_id(+)
      AND RoleManageEO.Person_Id = papf.person_id(+)
      AND TRUNC(SYSDATE) BETWEEN papf.effective_start_date AND papf.effective_end_date
   ]]></SQLQuery>
