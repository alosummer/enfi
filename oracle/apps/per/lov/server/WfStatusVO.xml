<?xml version='1.0' encoding='GBK' ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="WfStatusVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="cux.oracle.apps.per.lov.server.WfStatusVORowImpl"
   ComponentClass="cux.oracle.apps.per.lov.server.WfStatusVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT lv.lookup_code AS epm_phase_code
     , lv.meaning     AS epm_phase_name
  FROM fnd_lookup_values lv
 WHERE lv.lookup_type = 'EPM_FLOW_STATUS'
   AND lv.LANGUAGE = userenv('LANG')
   AND nvl(lv.start_date_active,SYSDATE)<=SYSDATE
   AND nvl(lv.end_date_active,SYSDATE)>=SYSDATE
   AND lv.enabled_flag='Y'
   AND lv.lookup_code IN ('COMPLETED','PUBLISHED','FREEZED')
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_isCodegen" Value="true" />
      <Attr Name="_isExpertMode" Value="true" />
      <Attr Name="_version" Value="10.1.3.39.81" />
      <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess" />
   </DesignTime>
   <ViewAttribute
      Name="EpmPhaseCode"
      IsPersistent="false"
      IsNotNull="true"
      Precision="30"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="EPM_PHASE_CODE"
      Expression="EPM_PHASE_CODE"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="30" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="EpmPhaseName"
      IsPersistent="false"
      IsNotNull="true"
      Precision="80"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="EPM_PHASE_NAME"
      Expression="EPM_PHASE_NAME"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="80" />
      </DesignTime>
   </ViewAttribute>
</ViewObject>
