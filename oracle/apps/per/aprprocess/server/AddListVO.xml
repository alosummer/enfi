<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="AddListVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="cux.oracle.apps.per.aprprocess.server.AddListVOImpl"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT ContractEO.APPRAISAL_ID,
       ContractEO.CONTRACT_ID,
       ContractEO.Kpi_Id,
       ContractEO.Kpi_Class,
       ContractEO.KPI_NAME,
       ContractEO.KPI_AREA,
       ContractEO.KPI_DESC,
       ContractEO.ATTRIBUTE5,              
       ContractEO.SCORE_VALUE
FROM CUX_CONTRACT_T ContractEO
WHERE 1 = 1
  AND ContractEO.Kpi_Class = 'ADD'      
  AND ContractEO.Appraisal_Id = :1
ORDER BY ContractEO.CONTRACT_ID
   ]]></SQLQuery>
