select t.project_id,t.segment1,t.name from pa_projects_all t
where 
cux_hr_bonus.GET_PROJECT_MANAGER_USERID(t.project_id)=fnd_global.user_id and
 t.Project_Status_Code!='CLOSED'
   ]]></SQLQuery>
