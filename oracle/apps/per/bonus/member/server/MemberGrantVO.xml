<ViewObject
   Name="MemberGrantVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="cux.oracle.apps.per.bonus.member.server.MemberGrantVORowImpl"
   ComponentClass="cux.oracle.apps.per.bonus.member.server.MemberGrantVOImpl"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select distinct
t.person_distribution_id,
ppf.employee_number,
ppf.last_name ,
t.dep_name,
t.distribution_amount,
t.distribution_date,
t.project_id,
ppa.segment1 project_number,
ppa.name project_name,
t.note,
case when t.import_status='已退回'
then '已退回'
else
t.distribution_status
end distribution_status,
t.import_status,
t.attribute1
from cux_bonus_person_distribution t,
cux_bonus_all_people_f_v  ppf,
pa_projects_all ppa
where  t.person_id=ppf.person_id
and cux_hr_bonus.GET_PROJECT_MANAGER_USERID(t.project_id)=
fnd_global.user_id 
and t.attribute3='ME'
and t.project_id=ppa.project_id
and t.distribution_status!='已删除'
order by  
nlssort(ppf.last_name, 'NLS_SORT=SCHINESE_PINYIN_M'),
ppf.employee_number,
t.distribution_date
   ]]></SQLQuery>
