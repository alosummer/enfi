SELECT distinct SpecialAwardsTypeEO.TYPE_ID,
       SpecialAwardsTypeEO.TYPE_NAME
  FROM CUX_BONUS_TYPE SpecialAwardsTypeEO
 WHERE valid_flag = 'Y'
   AND bonus_type IN ('预支奖', '产值奖')
   ]]></SQLQuery>
