<ViewObject
   Name="SpecialAwardsPersonVO"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="cux.oracle.apps.per.bonus.specialawards.server.SpecialAwardsPersonVORowImpl"
   ComponentClass="cux.oracle.apps.per.bonus.specialawards.server.SpecialAwardsPersonVOImpl"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT DISTINCT (person_distribution_id),
                employee_number,
                last_name,
                lot_id,
                cd.person_id,
                ct.type_name,
                cd.type_id,
                distribution_dep_id,
                houv.name as distributiondep_name,
                dep_id,
                dep_name,
                distribution_status,
                submit_flg,
                import_flag,
                import_status,
                import_error,
                distribution_date,
                distribution_amount,
                project_id,
                note,
                DECODE(distribution_status, '已批准', 'pDeleteDisabled', '处理中', 'pDeleteDisabled', 'pDeleteEnabled') deleteStatus,
                approved_note,
                cd.last_update_date,
                cd.last_updated_by,
                cd.creation_date,
                cd.created_by,
                cd.attribute1,
                cd.attribute2,
                cd.attribute3,
                cd.attribute4,
                cd.attribute5,
                cd.attribute6,
                cd.attribute7,
                cd.attribute8,
                cd.attribute9,
                cd.attribute10,
                cd.attribute11,
                cd.attribute12,
                cd.attribute13,
                cd.attribute14,
                cd.attribute15
  FROM CUX_BONUS_PERSON_DISTRIBUTION cd,
       PER_ALL_PEOPLE_F              pf,
       CUX_BONUS_TYPE                ct,
       CUX_BONUS_DEPT_V              houv
 WHERE cd.person_id = pf.person_id
   AND cd.type_id = ct.type_id
   AND cd.distribution_status NOT IN ('已删除', '已退回')
   AND cd.organization_id = houv.organization_id(+)
ORDER BY nlssort(last_name, 'NLS_SORT=SCHINESE_PINYIN_M')
   ]]></SQLQuery>
