<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="SourceTypeListVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="cux.oracle.apps.cux.qhse.rectify.poplist.server.SourceTypeListVORowImpl"
   ComponentClass="cux.oracle.apps.cux.qhse.rectify.poplist.server.SourceTypeListVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT pal2.lookup_code,
       pal2.meaning
  FROM pa_lookups pal2
 WHERE 1 = 1
   AND pal2.lookup_type = 'PA_CI_SOURCE_TYPES'
   AND pal2.enabled_flag = 'Y'
   AND nvl(pal2.start_date_active,
           SYSDATE) <= SYSDATE
   AND nvl(pal2.end_date_active,
           SYSDATE + 1) > SYSDATE
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_isCodegen" Value="true" />
      <Attr Name="_isExpertMode" Value="true" />
      <Attr Name="_version" Value="10.1.3.43.6" />
      <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess" />
   </DesignTime>
   <ViewAttribute
      Name="LookupCode"
      IsPersistent="false"
      IsNotNull="true"
      Precision="30"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="LOOKUP_CODE"
      Expression="LOOKUP_CODE"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="30" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="Meaning"
      IsPersistent="false"
      IsNotNull="true"
      Precision="80"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="MEANING"
      Expression="MEANING"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="80" />
      </DesignTime>
   </ViewAttribute>
</ViewObject>
