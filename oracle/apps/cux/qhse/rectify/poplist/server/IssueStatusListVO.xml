<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="IssueStatusListVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="cux.oracle.apps.cux.qhse.rectify.poplist.server.IssueStatusListVORowImpl"
   ComponentClass="cux.oracle.apps.cux.qhse.rectify.poplist.server.IssueStatusListVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT pal1.lookup_code,
       pal1.meaning
  FROM pa_lookups pal1
 WHERE 1 = 1
   AND pal1.lookup_type = 'ISSUE_CHANGE_STATUS'
   AND pal1.enabled_flag = 'Y'
   AND nvl(pal1.start_date_active,
           SYSDATE) <= SYSDATE
   AND nvl(pal1.end_date_active,
           SYSDATE + 1) > SYSDATE
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_isCodegen" Value="true" />
      <Attr Name="_isExpertMode" Value="true" />
      <Attr Name="_version" Value="10.1.3.43.6" />
      <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess" />
   </DesignTime>
   <ViewAttribute
      Name="LookupCode"
      IsPersistent="false"
      IsNotNull="true"
      Precision="30"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="LOOKUP_CODE"
      Expression="LOOKUP_CODE"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="30" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="Meaning"
      IsPersistent="false"
      IsNotNull="true"
      Precision="80"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="MEANING"
      Expression="MEANING"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="80" />
      </DesignTime>
   </ViewAttribute>
</ViewObject>
