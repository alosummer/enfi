<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="YesNoListVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="cux.oracle.apps.cux.qhse.rectify.poplist.server.YesNoListVORowImpl"
   ComponentClass="cux.oracle.apps.cux.qhse.rectify.poplist.server.YesNoListVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT lookup_code,
       meaning,
       description,
       view_application_id
  FROM fnd_lookup_values_vl
 WHERE fnd_lookup_values_vl.lookup_type = 'YES_NO'
   AND enabled_flag = 'Y'
   AND view_application_id = 0
   AND SYSDATE BETWEEN nvl(start_date_active,
                           SYSDATE - 1) AND
       nvl(end_date_active,
           SYSDATE + 1)
   ]]></SQLQuery>
   <DesignTime>
