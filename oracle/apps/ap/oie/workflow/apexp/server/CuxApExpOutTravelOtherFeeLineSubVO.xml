   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT caeoe.other_fee_line_id,
       caeoe.relationship_id,
       caeoe.emp_id,
       caeoe.fee_type,
       caeoe.fee_date,
       caeoe.fee_amount,
