       caeoe.last_update_login,
       caeoe.attribute_category,
       caeoe.attribute1,
       caeoe.attribute2,
       caeoe.attribute3,
       caeoe.attribute4,
       caeoe.attribute5,
       caeoe.attribute6,
       caeoe.attribute7,
       caeoe.attribute8,
       caeoe.attribute9,
       caeoe.attribute10,
       caeoe.attribute11,
       caeoe.attribute12,
       caeoe.attribute13,
       caeoe.attribute14,
       caeoe.attribute15,
       caeoe.line_number,
       caeoe.aboard_flag,
       caeoe.CURRENCY_CODE,
       caeoe.RATE,
       ppx.full_name AS emp_name,
       ppx.employee_number
  FROM cux_ap_exp_other_fee       caeoe,
       per_people_x               ppx
 WHERE caeoe.emp_id = ppx.person_id
   AND caeoe.aboard_flag = 'Y'
   ]]></SQLQuery>
