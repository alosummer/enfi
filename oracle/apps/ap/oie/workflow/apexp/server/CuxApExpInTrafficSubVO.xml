   Name="CuxApExpInTrafficSubVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="cux.oracle.apps.ap.oie.workflow.apexp.server.CuxApExpInTrafficSubVORowImpl"
   ComponentClass="cux.oracle.apps.ap.oie.workflow.apexp.server.CuxApExpInTrafficSubVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT catr.traffic_reimb_line_id,
       catr.relationship_id,
       catr.emp_id,
       catr.fee_type,
       catr.start_date,
       catr.end_date,
       catr.start_site,
       catr.end_site,
       catr.traffic_type,
       catr.traffic_number,
       catr.traffic_fee,
       catr.buy_ticket_fee,
       cux_ap_exp_pkg.get_emp_traffic_level_subsidy(p_traffic_reimb_line_id => catr.traffic_reimb_line_id) AS level_subsidy,
       cux_ap_exp_pkg.get_overtime_subsidy(p_traffic_reimb_line_id => catr.traffic_reimb_line_id) AS overtime_subsidy,
       catr.public_traffic_fee_in_jing,
       catr.taxi_fee_in_jing,
       catr.total_traffic_fee_out_jing,
       catr.remark,
       catr.creation_date,
       catr.created_by,
       catr.last_update_date,
       catr.last_updated_by,
       catr.last_update_login,
       catr.attribute_category,
       catr.attribute1,
       catr.attribute2,
       catr.attribute3,
       catr.attribute4,
       catr.attribute5,
       catr.attribute6,
       catr.attribute7,
       catr.attribute8,
       catr.attribute9,
       catr.attribute10,
       catr.attribute11,
       catr.attribute12,
       catr.attribute13,
       catr.attribute14,
       catr.attribute15,
       catr.line_number,
       ppx.full_name AS emp_name,
       ppx.employee_number,
       1 AS show_detail
  FROM cux_ap_exp_traffic_reimb catr,
       per_people_x             ppx
 WHERE catr.emp_id = ppx.person_id
   ]]></SQLQuery>
