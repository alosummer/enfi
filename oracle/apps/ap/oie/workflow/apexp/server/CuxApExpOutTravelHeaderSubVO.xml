   ComponentClass="cux.oracle.apps.ap.oie.workflow.apexp.server.CuxApExpOutTravelHeaderSubVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT caro.relationship_id,
       caro.report_header_id,
       caro.oa_travel_process_id,
       caro.oa_outsea_travel_process_id,
       caro.creation_date,
       caro.created_by,
       caro.last_update_date,
       caro.last_updated_by,
       caro.last_update_login,
       caro.attribute_category,
       caro.attribute1,
       caro.attribute2,
       caro.attribute3,
       caro.attribute4,
       caro.attribute5,
       caro.attribute6,
       caro.attribute7,
       caro.attribute8,
       caro.attribute9,
       caro.attribute10,
       caro.attribute11,
       caro.attribute12,
       caro.attribute13,
       caro.attribute14,
       caro.attribute15,
       caeata.oa_abroad_travel_header_name,
       caro.abourd_start_date,
       caro.abourd_end_date,
       (caro.abourd_end_date - caro.abourd_start_date) AS travel_days_number
  FROM cux_ap_exp_related_oa_info     caro,
       cux_ap_exp_abroad_travel_apy_v caeata
 WHERE caro.oa_outsea_travel_process_id = caeata.oa_abroad_travel_header_id(+)
   ]]></SQLQuery>
